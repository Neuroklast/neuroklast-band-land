    { "source": "/backup/:p*", "destination": "/api/denied?_src=/backup/:p*" },
    { "source": "/config/:p*", "destination": "/api/denied?_src=/config/:p*" },
    { "source": "/debug/:p*", "destination": "/api/denied?_src=/debug/:p*" },
    { "source": "/staging/:p*", "destination": "/api/denied?_src=/staging/:p*" },
    { "source": "/internal/:p*", "destination": "/api/denied?_src=/internal/:p*" },
    { "source": "/private/:p*", "destination": "/api/denied?_src=/private/:p*" },
    { "source": "/data/:p*", "destination": "/api/denied?_src=/data/:p*" },
    { "source": "/logs/:p*", "destination": "/api/denied?_src=/logs/:p*" },
    { "source": "/tmp/:p*", "destination": "/api/denied?_src=/tmp/:p*" },
